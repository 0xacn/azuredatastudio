{
	"name": "workspace-fun",
	"displayName": "Workspace Fun",
	"description": "Workspace Fun",
	"version": "1.0.0",
	"publisher": "anthonydresser",
	"license": "MIT",
	"engines": {
		"vscode": "^1.4.0"
	},
	"activationEvents": [
    "onCommand:workspaceFun.addFolder",
    "onCommand:workspaceFun.addJson",
    "onCommand:workspaceFun.addDataWorkspace",
    "onCommand:workspaceFun.addConnectionToWorkspace",
    "onFileSystem:json",
    "onFileSystem:dataworkspace"
	],
	"main": "./out/extension",
	"dependencies": {
	},
	"contributes": {
    "commands": [
      {
        "command": "workspaceFun.addFolder",
        "title": "Add Folder To Workspace"
      },
      {
        "command": "workspaceFun.addJson",
        "title": "Add json as resource"
      },
      {
        "command": "workspaceFun.addDataWorkspace",
        "title": "Add data workspace file as resource"
      },
      {
        "command": "workspaceFun.addConnectionToWorkspace",
        "title": "Add connection to workspace"
      }
    ]
	},
	"devDependencies": {
		"@types/node": "^12.11.7"
	}
}
